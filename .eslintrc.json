  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-console": 0 ,
      "linebreak-style": ["error", "windows"],
      "no-alert": "off",
      "no-console": 0,
      "no-restricted-syntax": [
          "error",
          "LabeledStatement",
          "WithStatement"
        ]
  }
}
